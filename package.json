{
    "name": "how-i-met-your-tracker",
    "version": "2.1.0",
    "description": "Chrome extension for tracking detection with ML features",
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "@types/jest": "^29.5.11"
    },
    "jest": {
        "testEnvironment": "node",
        "roots": [
            "<rootDir>/tests"
        ],
        "testMatch": [
            "**/*.test.js"
        ],
        "collectCoverageFrom": [
            "modules/**/*.js",
            "!modules/fingerprint_interceptor.js"
        ],
        "moduleNameMapper": {
            "^./modules/(.*)$": "<rootDir>/modules/$1"
        },
        "transform": {},
        "setupFiles": [
            "<rootDir>/tests/setup.js"
        ]
    }
}